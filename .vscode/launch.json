  "version": "0.2.0",
  "configurations": [
    {
      "name": "solid:dev",
      "request": "launch",
      "cwd": "${workspaceFolder}/solid",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "deno:server:dev",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/local/bin/deno",
      "runtimeArgs": [
        "task",
        "dev:server"
      ],
      "type": "node"
    }
  ]
}
